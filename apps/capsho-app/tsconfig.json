{
  "extends": "@quasar/app-webpack/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "useUnknownInCatchVariables": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true
  },
  "inlineSourcemap": true,
  "inlineSources": true
}
