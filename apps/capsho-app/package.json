    "pinia": "^2.0.11",
    "pinia-plugin-persist": "^1.0.0",
    "quasar": "^2.6.0",
    "sanitize-html": "^2.7.0",
    "v-onboarding": "^2.0.1",
    "vue": "^3.0.0",
