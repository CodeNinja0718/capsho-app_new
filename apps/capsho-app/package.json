{
  "name": "capsho-app-quasar",
  "version": "0.2.3",
  "description": "Podcast Copywriter - creates episode title, description, show notes, email, social media captions, blog post, linkedin article and YouTube description from an audio file",
  "productName": "Dashboard - Capsho",
  "author": "Beck Bekmyradov <47065940+bekmuradov@users.noreply.github.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "yarn quasar dev",
    "build": "yarn quasar build",
    "deploy": "vercel",
    "start": "yarn quasar serve --history",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "e2e:test": "yarn playwright test",
    "e2e:codegen": "yarn playwright codegen http://localhost:8080/auth/login",
    "e2e:chrome": "yarn playwright test --project=chromium",
    "e2e:debug": "yarn playwright test --debug",
    "e2e:show:report": "npx playwright show-report"
  },
  "dependencies": {
    "@emailjs/browser": "^3.4.0",
